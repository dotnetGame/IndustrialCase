{
  "multipart": [
    {
      "apply": {
        "model": "industrialcase:block/wiring/cable/copper_cable_insulated_core"
      }
    },
    {
        "when": { "up": "true" },
        "apply": { "model": "industrialcase:block/wiring/cable/copper_cable_insulated_side", "x": -90, "uvlock": true }
    },

    {
        "when": { "down": "true" },
        "apply": { "model": "industrialcase:block/wiring/cable/copper_cable_insulated_side", "x": 90, "uvlock": true }
    },

    {
        "when": { "north": "true" },
        "apply": { "model": "industrialcase:block/wiring/cable/copper_cable_insulated_side", "uvlock": true }
    },
    {
        "when": { "east": "true" },
        "apply": { "model": "industrialcase:block/wiring/cable/copper_cable_insulated_side", "y": 90, "uvlock": true }
    },
    {
        "when": { "south": "true" },
        "apply": { "model": "industrialcase:block/wiring/cable/copper_cable_insulated_side", "y": 180, "uvlock": true }
    },
    {
        "when": { "west": "true" },
        "apply": { "model": "industrialcase:block/wiring/cable/copper_cable_insulated_side", "y": 270, "uvlock": true }
    }
  ]
}